#: avatar/admin.py:26
msgid "Avatar"
msgstr "头像"

#: avatar/forms.py:24
msgid "avatar"
msgstr "头像"

#: avatar/forms.py:37
#, python-format
msgid ""
"%(ext)s is an invalid file extension. Authorized extensions are : "
"%(valid_exts_list)s"
msgstr "%(ext)s 是不正确的文件扩展名。 正确的扩展名为 : %(valid_exts_list)s"

#: avatar/forms.py:44
#, python-format
msgid ""
"Your file is too big (%(size)s), the maximum allowed size is "
"%(max_valid_size)s"
msgstr "上传文件太大 (%(size)s)， 允许的最大文件为 %(max_valid_size)s"

#: avatar/forms.py:54
#, python-format
msgid ""
"You already have %(nb_avatars)d avatars, and the maximum allowed is "
"%(nb_max_avatars)d."
msgstr "您目前有 %(nb_avatars)d 个头像, 最多可以有 %(nb_max_avatars)d 个。"

#: avatar/forms.py:71 avatar/forms.py:84
msgid "Choices"
msgstr "选项"

#: avatar/templates/avatar/add.html:7 avatar/templates/avatar/change.html:7
msgid "Your current avatar: "
msgstr "您当前的头像："

#: avatar/templates/avatar/add.html:10 avatar/templates/avatar/change.html:10
msgid "You haven't uploaded an avatar yet. Please upload one now."
msgstr "您还没有上传任何头像，请现在上传一个吧。"

#: avatar/templates/avatar/add.html:14 avatar/templates/avatar/change.html:21
msgid "Upload New Image"
msgstr "上传新照片"

#: avatar/templates/avatar/change.html:16
msgid "Choose new Default"
msgstr "选择默认"

#: avatar/templates/avatar/confirm_delete.html:5
msgid "Please select the avatars that you would like to delete."
msgstr "选择要删除的头像。"

#: avatar/templates/avatar/confirm_delete.html:8
#, python-format
msgid ""
"You have no avatars to delete. Please <a href=\"%(avatar_change_url)s"
"\">upload one</a> now."
msgstr ""
"没有头像可以删除. 请 <a href=\"%(avatar_change_url)s\">上传一个新头像</a>。"

#: avatar/templates/avatar/confirm_delete.html:14
msgid "Delete These"
msgstr "删除"

#: avatar/templates/notification/avatar_friend_updated/full.txt:1
#, python-format
msgid ""
"%(avatar_creator)s has updated their avatar %(avatar)s.\n"
"\n"
"http://%(current_site)s%(avatar_url)s\n"
msgstr ""
"%(avatar_creator)s 更新了头像 %(avatar)s.\n"
"\n"
"http://%(current_site)s%(avatar_url)s\n"

#: avatar/templates/notification/avatar_friend_updated/notice.html:2
#, python-format
msgid ""
"<a href=\"%(user_url)s\">%(avatar_creator)s</a> has updated their avatar <a "
"href=\"%(avatar_url)s\">%(avatar)s</a>."
msgstr ""
"<a href=\"%(user_url)s\">%(avatar_creator)s</a> 更新了头像 <a href="
"\"%(avatar_url)s\">%(avatar)s</a>."

#: avatar/templates/notification/avatar_updated/full.txt:1
#, python-format
msgid ""
"Your avatar has been updated.  %(avatar)s\n"
"\n"
"http://%(current_site)s%(avatar_url)s\n"
msgstr ""
"您的头像已经更新。  %(avatar)s\n"
"\n"
"http://%(current_site)s%(avatar_url)s\n"

#: avatar/templates/notification/avatar_updated/notice.html:2
#, python-format
msgid "You have updated your avatar <a href=\"%(avatar_url)s\">%(avatar)s</a>."
msgstr "您已经更新了头像 <a href=\"%(avatar_url)s\">%(avatar)s</a>."

#: avatar/templatetags/avatar_tags.py:51
msgid "Default Avatar"
msgstr "默认头像"

#: avatar/views.py:74
msgid "Successfully uploaded a new avatar."
msgstr "成功上传头像。"

#: avatar/views.py:110
msgid "Successfully updated your avatar."
msgstr "更新头像成功。"

#: avatar/views.py:148
msgid "Successfully deleted the requested avatars."
msgstr "成功删除头像。"
